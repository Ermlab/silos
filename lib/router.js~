Router.configure({
layoutTemplate: 'layout',
/* 
waitOn: function() { return Meteor.subscribe('logi'); }
*/
});
Router.map(function() {
this.route('startPage', {path: '/'});
    
});

var requireLogin = function(pause){
 if (! Meteor.user()) {
	this.render('accessDenied');
	pause();
	}
 }

Router.onBeforeAction(requireLogin, {only: 'createLogBooks'});


